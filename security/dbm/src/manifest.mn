CORE_DEPTH = ../..

VPATH  = $(CORE_DEPTH)/../dbm/src

MODULE = dbm

#
# memmove.c, snprintf.c, and strerror.c are not in CSRCS because
# the Standard C Library has memmove and strerror and DBM is not
# using snprintf.
#

CSRCS = db.c	   \
	h_bigkey.c \
	h_func.c   \
	h_log2.c   \
	h_page.c   \
	hash.c	   \
	hash_buf.c \
	hsearch.c  \
	mktemp.c   \
	ndbm.c	   \
	dirent.c   \
	$(NULL)

LIBRARY_NAME = dbm
