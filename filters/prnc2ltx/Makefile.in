
DEPTH		= ../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

include $(topsrcdir)/config/rules.mk


XSL_RESOURCES := $(wildcard $(srcdir)/*.xsl)
XML_RESOURCES := $(wildcard $(srcdir)/*.xml)

libs::	$(XSL_RESOURCES)
	$(INSTALL)	$(XSL_RESOURCES)	$(FINAL_TARGET)/res/xsl
	$(INSTALL)	$(XML_RESOURCES)	$(FINAL_TARGET)/res/xsl
