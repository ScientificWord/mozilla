<!-- assignment of function keys to macros, mostly tag selection macros 
  Note: names are names of keys, not characters. The full name is gotten by prepending 'DOM_VK_', so
  the first key is DOM_VK_F1. -->

  <keytable id="FKeys" name="FKeys" keytype="script">
    <key name="F1"  vk="1">showhelpsystem();</key>
    <key name="F2"  vk="1">insertParaTag('bodyText',false);</key>
    <key name="F2"  shift="1" vk="1">insertParaTag('bodyMath',false);</key>
    <key name="F3"  vk="1">findNext();</key>
    <key name="F3"  shift="1" vk="1">findPrev();</key>
    <key name="F4"  vk="1">toggleTextTag('(normal)',false);</key>
    <key name="F4"  shift="1" vk="1">insertSectionTag('(removesection)',false);</key>
    <key name="F5"  vk="1">toggleTextTag('bold',false);</key>
    <key name="F5"  shift="1" vk="1">toggleTextTag('boldsymbol',false);</key>
    <key name="F6"  vk="1">toggleTextTag('emphasized',false);</key>
    <key name="F6"  shift="1" vk="1">toggleTextTag('italics',false);</key>
    <key name="F7"  vk="1">insertTag('numberedListItem',false);</key>
    <key name="F8"  vk="1">insertTag('bulletListItem',false);</key>
    <key name="F9"  vk="1">toggleTextTag('typewriter',false);</key>
    <key name="F9"  shift="1" vk="1">toggleTextTag('roman',false);</key>
    <key name="F11"  vk="1">insertSectionTag('section',false);</key>
    <key name="F11"  alt="1" vk="1">softSave();</key>
    <key name="F11"  shift="1" vk="1">insertSectionTag('chapter',false);</key>
    <key name="F12"  vk="1">insertSectionTag('subsection',false);</key>
    <key name="F12"  shift="1" vk="1">insertSubstructure(false);</key>
	<!-- Plot function key shortcuts -->
    <key name="F1"  alt="1" vk="1">doComputeCommand('cmd_compute_Plot2DRectangular');</key>
    <key name="F1"  alt="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DRectangular');</key>
    <key name="F1"  ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot2DARectangular');</key>
    <key name="F1"  alt="1" ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DARectangular');</key>
    <key name="F2"  alt="1" vk="1">doComputeCommand('cmd_compute_Plot2DPolar');</key>
    <key name="F2"  alt="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DCurve');</key>
    <key name="F2"  ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot2DAPolar');</key>
    <key name="F2"  alt="1" ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DACurve');</key>
    <key name="F3"  alt="1" vk="1">doComputeCommand('cmd_compute_Plot2DImplicit');</key>
    <key name="F3"  alt="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DImplicit');</key>
    <key name="F3"  ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot2DAImplicit');</key>
    <key name="F3"  alt="1" ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DAImplicit');</key>
    <key name="F4"  alt="1" vk="1">doComputeCommand('cmd_compute_Plot2DInequality');</key>
    <key name="F4"  ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot2DAInequality');</key>
    <key name="F5"  alt="1" vk="1">doComputeCommand('cmd_compute_Plot2DParametric');</key>
    <key name="F5"  alt="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DParametric');</key>
    <key name="F5"  ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot2DAParametric');</key>
    <key name="F5"  alt="1" ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DAParametric');</key>
    <key name="F6"  alt="1" vk="1">doComputeCommand('cmd_compute_Plot2DGradient');</key>
    <key name="F6"  alt="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DGradient');</key>
    <key name="F6"  ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot2DAGradient');</key>
    <key name="F6"  alt="1" ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DAGradient');</key>
    <key name="F7"  alt="1" vk="1">doComputeCommand('cmd_compute_Plot2DVector');</key>
    <key name="F7"  alt="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DVector');</key>
    <key name="F7"  ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot2DAVector');</key>
    <key name="F7"  alt="1" ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DAVector');</key>
    <key name="F8"  alt="1" vk="1">doComputeCommand('cmd_compute_Plot2DConformal');</key>
    <key name="F8"  alt="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DCylindrical');</key>
    <key name="F8"  ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot2DAConformal');</key>
    <key name="F8"  alt="1" ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DACylindrical');</key>
    <key name="F9"  alt="1" vk="1">doComputeCommand('cmd_compute_Plot2DODE');</key>
    <key name="F9"  alt="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DSpherical');</key>
    <key name="F9"  alt="1" ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DASpherical');</key>
    <key name="F10"  alt="1" vk="1">doComputeCommand('cmd_compute_Plot2DAI');</key>
    <key name="F10"  alt="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DTube');</key>
    <key name="F10"  alt="1" ctrl="1" shift="1" vk="1">doComputeCommand('cmd_compute_Plot3DATube');</key>
  </keytable>

