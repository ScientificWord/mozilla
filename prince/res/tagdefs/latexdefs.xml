      <tags>
        <tag id="clearsectiontag" nm="(removesection)">
          <description>Remove innermost section-like tag</description>
        </tag>
        <tag nm="set">
          <level>-3</level>
          <description>Set of books</description>
          <discardemptyblock>true</discardemptyblock>
          <inclusion>true</inclusion>
          <contains>bulletListItem</contains>
        </tag>
        <tag nm="book">
          <level>-2</level>
          <description>Book</description>
          <titletag>sectiontitle</titletag>
          <discardemptyblock>false</discardemptyblock>
          <prefsub>part</prefsub>
        </tag>
        <tag nm="part">
          <level>-1</level>
          <description>Part of a book</description>
          <titletag>sectiontitle</titletag>
          <discardemptyblock>false</discardemptyblock>
          <prefsub>chapter</prefsub>
        </tag>
        <tag nm="chapter">
          <level>1</level>
          <description>Chapter</description>
          <titletag>sectiontitle</titletag>
          <discardemptyblock>false</discardemptyblock>
          <initialcontents>
            <chapter>
              <sectiontitle>
                <cursor/>
                <br/>
              </sectiontitle>
            </chapter>
          </initialcontents>
          <prefsub>section</prefsub>
        </tag>
        <tag nm="section">
          <level>2</level>
          <description>Section</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents>
            <section>
              <sectiontitle>
                <cursor/>
                <br/>
              </sectiontitle>
            </section>
          </initialcontents>
          <nextafteremptyblock/>
          <prefsub>subsection</prefsub>
        </tag>
        <tag nm="subsection">
          <level>3</level>
          <description>Subsection</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents>
            <subsection>
              <sectiontitle>
                <cursor/>
                <br/>
              </sectiontitle>
            </subsection>
          </initialcontents>
          <nextafteremptyblock>section</nextafteremptyblock>
          <prefsub>subsubsection</prefsub>
        </tag>
        <tag nm="subsubsection">
          <level>4</level>
          <description>Subsubsection</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents>
            <subsubsection>
              <sectiontitle>
                <cursor/>
                <br/>
              </sectiontitle>
            </subsubsection>
          </initialcontents>
          <nextafteremptyblock>subsection</nextafteremptyblock>
          <prefsub>paragraph</prefsub>
        </tag>
        <tag nm="paragraph">
          <level>5</level>
          <description>Subsubsubsubsection</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents>
            <paragraph>
              <sectiontitle>
                <cursor/>
                <br/>
              </sectiontitle>
            </paragraph>
          </initialcontents>
          <nextafteremptyblock>subsubsection</nextafteremptyblock>
          <prefsub>subparagraph</prefsub>
        </tag>
        <tag nm="subparagraph">
          <level>6</level>
          <description>Section (dynamic level)</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents>
            <subparagraph>
              <sectiontitle>
                <cursor/>
                <br/>
              </sectiontitle>
            </subparagraph>
          </initialcontents>
          <nextafteremptyblock>paragraph</nextafteremptyblock>
        </tag>
      </tags>
    </tagclass>
