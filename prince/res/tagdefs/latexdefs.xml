				<tag nm="(removesection)" id="clearsectiontag" ><description>Remove innermost section-like tag</description></tag>
				<tag nm="set">
        	<level>-3</level>
          <description>Set of books</description>
          <discardemptyblock>true</discardemptyblock>
	  		  <inclusion>true</inclusion>
			    <contains>bulletListItem</contains>
        </tag>
				<tag nm="book">
          <level>-2</level>
          <description>Book</description>
          <titletag>sectiontitle</titletag>
          <discardemptyblock>false</discardemptyblock>
          <prefsub>part</prefsub>
        </tag>
				<tag nm="part">
          <level>-1</level>
          <description>Part of a book</description>
          <titletag>sectiontitle</titletag>
          <discardemptyblock>false</discardemptyblock>
          <prefsub>chapter</prefsub>
        </tag>
				<tag nm="chapter">
          <level>1</level>
          <description>Chapter</description>
          <titletag>sectiontitle</titletag>
          <discardemptyblock>false</discardemptyblock>
          <initialcontents>
            <chapter>
              <sectiontitle>
                <cursor/>
                <br/>
              </sectiontitle>
            </chapter>
          </initialcontents>
          <prefsub>section</prefsub>
        </tag>
				<tag nm="section">
          <level>2</level>
          <description>Section</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents>
             <section>
               <sectiontitle>
                 <cursor/>
                 <br/>
               </sectiontitle>
             </section>
           </initialcontents>
          <nextafteremptyblock>chapter</nextafteremptyblock>
          <prefsub>subsection</prefsub>
        </tag>
				<tag nm="subsection">
          <level>3</level>
          <description>Subsection</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
