			<tags>
				<tag id="defaultparagraph" nm="bodyText">
					<description>Default body text paragraph</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <bodyText>
              <cursor/>
              <br/>
            </bodyText>
          </initialcontents>
				</tag>
				<tag nm="rtlBodyText">
					<description>Right to left body text paragraph</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <rtlBodyText>
              <cursor/>
              <br/>
            </rtlBodyText>
          </initialcontents>
				</tag>
				<tag nm="p">
					<description>HTML paragraph</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <p>
              <cursor/>
              <br/>
            </p>
          </initialcontents>
				</tag>
				<tag nm="bodyMath">
					<description>Body math paragraph</description>
					<nexttag>bodyMath</nexttag>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <bodyMath>
              <mml:math>
                <mml:mi tempinput='true'>
                  <cursor/>&#x200B;
                </mml:mi>
              </mml:math>
              <br/>
            </bodyMath>
          </initialcontents>
				</tag>
				<tag nm="sectiontitle">
					<nexttag>bodyText</nexttag>
					<description>Title of section</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <sectiontitle>
              <cursor/>
              <br/>
            </sectiontitle>
          </initialcontents>
				</tag>
				<tag nm="shortQuote">
					<nexttag>shortQuote</nexttag>
					<description>Quote</description>
          <initialcontents>
            <shortQuote>
              <cursor/>
              <br/>
            </shortQuote>
          </initialcontents>
          <discardemptyblock>true</discardemptyblock>
          <nextafteremptyblock>bodyText</nextafteremptyblock>        
			  </tag>
				<tag nm="line">
					<nexttag>line</nexttag>
					<description>Line</description>
          <initialcontents>
