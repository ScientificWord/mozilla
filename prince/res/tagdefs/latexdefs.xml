        </tag>
				<tag ns="sw" nm="appendix">
          <level> 0</level>
          <description>Appendix</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <p></p>
              ]]>
          </initialcontents>
        </tag>
				<tag ns="sw" nm="glossary">
          <level> 0</level>
          <description>Glossary</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <p></p>
              ]]>
          </initialcontents>
          <initialcontents> <![CDATA[
              <p></p>
              ]]>
          </initialcontents>
        </tag>
				<tag ns="sw" nm="bibliography">
          <level> 0</level>
          <description>Bibliography</description>
          <discardemptyblock>true</discardemptyblock>
        </tag>
				<tag ns="sw" nm="refsect1">
          <level> 1</level>
          <description>Refsect1</description>
          <discardemptyblock>true</discardemptyblock>
