    <tagclass>
      <tagclassname>listparenttag</tagclassname>
      <classcontains/>
      <tags>
        <tag hidden="1" nm="bulletlist">
          <description>Bullet list</description>
          <nexttag>none</nexttag>
          <inclusion>true</inclusion>
          <htmllist>ul</htmllist>
          <discardemptyblock>true</discardemptyblock>
          <contains>bulletListItem</contains>
        </tag>
        <tag hidden="1" nm="numberedlist">
          <description>Numbered list</description>
          <nexttag>none</nexttag>
          <htmllist>ol</htmllist>
          <initialcontents><![CDATA[
            <cursor/>
            <br/>
            ]]></initialcontents>
          <discardemptyblock>true</discardemptyblock>
          <inclusion>true</inclusion>
          <contains>numberedListItem</contains>
        </tag>
        <tag hidden="1" nm="descriptionlist">
          <description>Description list</description>
          <nexttag>none</nexttag>
          <htmllist>dl</htmllist>
          <discardemptyblock>true</discardemptyblock>
          <inclusion>true</inclusion>
          <nexttag/>
          <contains>descriptionListItem</contains>
        </tag>
        <tag hidden="1" nm="bibliography">
          <level>0</level>
          <description>Bibliography</description>
          <nexttag>none</nexttag>
          <htmllist>ul</htmllist>
          <discardemptyblock>true</discardemptyblock>
          <inclusion>true</inclusion>
          <contains>bibitem</contains>
        </tag>
      </tags>
    </tagclass>
    <tagclass>
      <tagclassname>listtag</tagclassname>
      <classcontains>
        <contains>bodyText</contains>
      </classcontains>
      <tags>
        <tag nm="bulletListItem">
          <description>Bullet list item</description>
          <htmllistparent>bulletlist</htmllistparent>
          <discardemptyblock>true</discardemptyblock>
          <!--           <inclusion>true</inclusion>-->
          <htmllist>ul</htmllist>
          <initialcontents>
            <bulletListItem>
              <bodyText>
                <cursor/>
                <br/>
              </bodyText>
            </bulletListItem>
          </initialcontents>
        </tag>
        <tag nm="numberedListItem">
          <description>Numbered list item</description>
          <htmllistparent>numberedlist</htmllistparent>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <numberedListItem>
              <bodyText>
                <cursor/>
                <br/>
              </bodyText>
            </numberedListItem>
          </initialcontents>
        </tag>
        <tag nm="descriptionListItem">
          <description>Description list item</description>
          <htmllistparent>descriptionlist</htmllistparent>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <descriptionListItem>
              <bodyText>
                <descriptionLabel>&#x20;
                  <cursor/>
                </descriptionLabel>&#x20;
                <br/>
              </bodyText>
            </descriptionListItem>
          </initialcontents>
        </tag>
        <tag nm="bibitem">
          <description>Bibliography entry</description>
          <htmllistparent>bibliography</htmllistparent>
          <discardemptyblock>true</discardemptyblock>
          <!--           <inclusion>true</inclusion>-->
          <initialcontents>
            <bibitem>
              <biblabel/>
              <bibkey/>
              <bodyText>
                <br/>
              </bodyText>
            </bibitem>
          </initialcontents>
          <contains>biblabel</contains>
          <contains>bibkey</contains>
        </tag>
      </tags>
