<?xml version="1.0"?>
<!DOCTYPE tagproperties [
<!ATTLIST tag id ID #IMPLIED> ]>
<tagproperties> 
	<forshell> <!-- I'm not sure this information is useful; consider it provisional -->
		StandardLatexArticle
	</forshell>
	<namespaces>
    <!-- currently white space in the namespace declarations will be kept, so these declarations
      should be on one line with no white space -->
	  <ns abbrev="sw">http://www.sciword.com/namespaces/sciword</ns>
    <ns abbrev="html">http://www.w3.org/1999/xhtml</ns>
	  <ns abbrev="mml">http://www.w3.org/1998/Math/MathML</ns>
	</namespaces>
	<tagclasses>
		<tagclass>
			<tagclassname>hidden</tagclassname>
			<contains>texttag</contains>
			<contains>paratag</contains>
			<contains>structtag</contains>
			<contains>othertag</contains>
			<tags>
				<tag nm="html:body"><description>Body</description></tag>
				<tag nm="sw:docbody"><description>DocBody</description></tag>
        <tag nm="html:img"><description>Image</description></tag>
			</tags>
		</tagclass>
		<tagclass>
			<tagclassname>texttag</tagclassname>
      <contains>#text</contains>
      <contains>table</contains>
			<contains>texttag</contains>
			<contains>othertag</contains>
			<contains>mml:math</contains>
			<contains>html:img</contains>
      <contains>line</contains>
			<tags>
				<tag nm="sw:(normal)" id="cleartexttag" ><description>Clear text tags</description></tag>
				<tag nm="sw:bold"><description>Bold</description></tag>
				<tag nm="sw:bbold"><description>Blackboard Bold</description></tag>
				<tag nm="sw:cal"><description>Calligraphic</description></tag>
				<tag nm="sw:em"><description>Emphasized</description></tag>
				<tag nm="sw:footnotesize"><description>Footnote size</description></tag>
				<tag nm="sw:otfont" hidden="1"><description>Open TypeFont</description></tag>
				<tag nm="sw:fontsize" hidden="1"><description>Font size</description></tag>
				<tag nm="sw:fontcolor" hidden="1"><description>Font color</description></tag>
				<tag nm="sw:fraktur"><description>Fraktur</description></tag>
				<tag nm="sw:rtl"><description>Right-to-left font</description></tag>
				<tag nm="sw:Huge"><description>Huge</description></tag>
				<tag nm="sw:huge"><description>huge</description></tag>
				<tag nm="sw:it"><description>Italics</description></tag>
				<tag nm="sw:LARGE"><description>LARGE</description></tag>
				<tag nm="sw:Large"><description>Large</description></tag>
				<tag nm="sw:large"><description>large</description></tag>
				<tag nm="sw:lower"><description>Lower case</description></tag>
				<tag nm="sw:normalsize"><description>normalsize</description></tag>
				<tag nm="sw:rm"><description>Roman</description></tag>
				<tag nm="sw:sf"><description>Sans Serif</description></tag>
				<tag nm="sw:scriptsize"><description>Scriptsize</description></tag>
				<tag nm="sw:sl"><description>Slanted</description></tag>
				<tag nm="sw:small"><description>Small</description></tag>
				<tag nm="sw:sc"><description>Small Caps</description></tag>
				<tag nm="sw:tiny"><description>Tiny</description></tag>
				<tag nm="sw:tt"><description>Typewriter font</description></tag>
				<tag nm="sw:upper"><description>Upper case</description></tag>
				<tag nm="sw:footnote">
          <level>*</level>
          <description>Footnote</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <para><br/></para>
              ]]>
          </initialcontents>
        </tag>
				<tag nm="sw:marginnote">
          <level>*</level>
          <description>Margin note</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <para><br/></para>
              ]]>
          </initialcontents>
        </tag>
			</tags>
		</tagclass>
		<tagclass>
			<tagclassname>paratag</tagclassname>
      <contains>#text</contains>
      <contains>table</contains>
			<contains>texttag</contains>
			<contains>listtag</contains>
			<contains>sw:quote</contains>
			<contains>othertag</contains>
			<contains>mml:math</contains>
			<contains>html:br</contains>
			<contains>sw:quote</contains>
			<contains>cursor</contains>
			<contains>sw:defaulttitletag</contains>
			<contains>html:img</contains>
      <contains>line</contains>
			<tags>
				<tag nm="sw:address">
          <nexttag>sw:para</nexttag>
          <description>Author address</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontentsforempty> <![CDATA[<cursor/><br temp="1"/>]]></initialcontentsforempty>
        </tag>
				<tag id="defaultparagraph" nm="sw:para">
					<description>Default body text paragraph</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontentsforempty> <![CDATA[<hspace type="zeroSpace" dim="0.0em" /><cursor/>]]>
          </initialcontentsforempty>
				</tag>
				<tag nm="sw:rtlpara">
					<description>Right to left body text paragraph</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontentsforempty> <![CDATA[<cursor/><br temp="1"/>]]>
          </initialcontentsforempty>
				</tag>
				<tag nm="sw:author">
          <nexttag>sw:para</nexttag>
          <description>Author name</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontentsforempty> <![CDATA[<cursor/><br temp="1"/>]]></initialcontentsforempty>
        </tag>
				<tag nm="html:p">
					<description>HTML paragraph</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontentsforempty> <![CDATA[<cursor/>]]>
          </initialcontentsforempty>
				</tag>
				<tag nm="sw:mathp">
					<description>Body math paragraph</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontentsforempty> <![CDATA[<math xmlns="http://www.w3.org/1998/Math/MathML"><cursor/></math>]]>
          </initialcontentsforempty>
				</tag>
				<tag nm="sw:sectiontitle">
					<nexttag>sw:para</nexttag>
					<description>Title of section</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[<defaulttitletag/><cursor/><br temp="1"/>]]>
          </initialcontents>
				</tag>
				<tag nm="sw:quote">
					<nexttag>sw:quote</nexttag>
					<description>Quote</description>
          <initialcontentsforempty> <![CDATA[<cursor/><br temp="1"/>]]>
          </initialcontentsforempty>
			  </tag>
				<tag nm="sw:quotation">
					<description>Long quotation</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontentsforempty> <![CDATA[<cursor/>]]>
          </initialcontentsforempty>
          <nextafteremptyblock>para</nextafteremptyblock>
				</tag>
				<tag nm="sw:title">
					<nexttag>sw:para</nexttag>
					<description>Title</description>
          <initialcontents> <![CDATA[<defaulttitletag/><cursor/><br temp="1"/>]]>
          </initialcontents>
				</tag>
				<tag nm="sw:line">
					<nexttag>sw:line</nexttag>
					<description>Line</description>
          <initialcontents> <![CDATA[<cursor/><br temp="1"/>]]>
          </initialcontents>
				</tag>
				<tag nm="sw:dialogbase" hidden="1">
					<nexttag>sw:para</nexttag>
					<description>Hidden dialog base</description>
          <initialcontents> <![CDATA[<cursor/><br temp="1"/>]]>
          </initialcontents>
				</tag>
			</tags>
		</tagclass>
		<tagclass>
			<tagclassname>listtag</tagclassname>
			<contains>texttag</contains>
			<contains>paratag</contains>
			<contains>mml:math</contains>
			<contains>sw:xref</contains>
			<contains>html:br</contains>
      <contains>line</contains>
			<tags>
				<tag nm="sw:itemizedlistitem">
          <description>Bulleted list time</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <para><cursor/></para>
              ]]>
          </initialcontents>
        </tag>
				<tag nm="sw:orderedlistitem">
          <description>Numbered list item</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <para><cursor/></para>
              ]]>
          </initialcontents>
        </tag>
				<tag nm="sw:simplelistitem">
          <description>Simple list item</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <para><cursor/></para>
              ]]>
          </initialcontents>
        </tag>
			</tags>
		</tagclass>
		<tagclass>
			<tagclassname>structtag</tagclassname>
			<contains>paratag</contains>
			<contains>structtag</contains>
			<contains>sw:xref</contains>
			<contains>html:br</contains>
			<tags>
				<tag nm="sw:set">
        	<level>-3</level>
          <description>Set of books</description>
          <discardemptyblock>true</discardemptyblock>
        </tag>
				<tag nm="sw:book">
          <level>-2</level>
          <description>Book</description>
          <discardemptyblock>true</discardemptyblock>
          <prefsub>part</prefsub>
        </tag>
				<tag nm="sw:part">
          <level>-1</level>
          <description>Part of a book</description>
          <discardemptyblock>true</discardemptyblock>
          <prefsub>chapter</prefsub>
        </tag>
				<tag nm="sw:chapter">
          <level>1</level>
          <description>Chapter</description>
          <titletag>sectiontitle</titletag>
          <discardemptyblock>false</discardemptyblock>
          <initialcontents> <![CDATA[<sectiontitle><cursor/><br temp="1"/></sectiontitle>]]>
          </initialcontents>
          <prefsub>section</prefsub>
        </tag>
				<tag nm="sw:section">
          <level>2</level>
          <description>Section</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents> <![CDATA[<sectiontitle><cursor/><br temp="1"/></sectiontitle>]]>
          </initialcontents>
          <nextafteremptyblock>chapter</nextafteremptyblock>
          <prefsub>subsection</prefsub>
        </tag>
				<tag nm="sw:subsection">
          <level>3</level>
          <description>Subsubsection</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents> <![CDATA[<sectiontitle><cursor/><br temp="1"/></sectiontitle>]]>
          </initialcontents>
          <nextafteremptyblock>section</nextafteremptyblock>
          <prefsub>subsubsection</prefsub>
        </tag>
				<tag nm="sw:subsubsection">
          <level>4</level>
          <description>Subsubsection</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents> <![CDATA[<sectiontitle><cursor/><br temp="1"/></sectiontitle>]]>
          </initialcontents>
          <nextafteremptyblock>subsection</nextafteremptyblock>
          <prefsub>paragraph</prefsub>
        </tag>
				<tag nm="sw:paragraph">
          <level>5</level>
          <description>Subsubsubsubsection</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents> <![CDATA[<sectiontitle><cursor/><br temp="1"/></sectiontitle> ]]>
          </initialcontents>
          <nextafteremptyblock>subsubsection</nextafteremptyblock>
          <prefsub>subparagraph</prefsub>
        </tag>
				<tag nm="sw:subparagraph">
          <level>6</level>
          <description>Section (dynamic level)</description>
          <discardemptyblock>true</discardemptyblock>
          <titletag>sectiontitle</titletag>
          <initialcontents> <![CDATA[<sectiontitle><cursor/><br temp="1"/></sectiontitle>]]>
          </initialcontents>
          <nextafteremptyblock>paragraph</nextafteremptyblock>
        </tag>
				<tag nm="sw:appendix">
          <level>0</level>
          <description>Appendix</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <para></para>
              ]]>
          </initialcontents>
          <prefsub>section</prefsub>
        </tag>
			</tags>
		</tagclass>
		<tagclass>
			<tagclassname>envtag</tagclassname>
			<contains>paratag</contains>
			<contains>structtag</contains>
			<tags>
				<tag nm="sw:abstract">
          <level>*</level>
          <nexttag>para</nexttag>
          <description>Abstract</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[ <para><cursor/><br temp="1"/></para>]]>
          </initialcontents>
          <nextafteremptyblock>para</nextafteremptyblock>
        </tag>
				<tag nm="sw:dedication">
          <level>*</level>
          <description>Dedication</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <para><cursor/><br temp="1"/></para>
              ]]>
          </initialcontents>
        </tag>
				<tag nm="sw:preface">
          <level>0</level>
          <description>Preface</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <para></para>
              ]]>
          </initialcontents>
        </tag>
				<tag nm="sw:index">
          <level>*</level>
          <description>Index</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <para></para>
              ]]>
          </initialcontents>
        </tag>
				<tag nm="sw:glossary">
          <level>0</level>
          <description>Glossary</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents> <![CDATA[
              <para></para>
              ]]>
          </initialcontents>
        </tag>
				<tag nm="sw:bibliography">
          <level>0</level>
          <description>Bibliography</description>
          <discardemptyblock>true</discardemptyblock>
        </tag>
				<tag nm="sw:verbatim">
					<description>Verbatim text</description>
				</tag>
			</tags>
		</tagclass>
		<tagclass>
			<tagclassname>othertag</tagclassname>
			<tags>
				<tag nm="sw:xref"><description>An instance of an "other" tag</description></tag>
        <tag nm="sw:latex"><description>LaTeX logo</description></tag>
        <tag nm="sw:tex"><description>TeX logo</description></tag>
        <tag nm="sw:maketitle"><description>Make title here</description></tag>
			</tags>
		</tagclass>
	</tagclasses>
</tagproperties>
