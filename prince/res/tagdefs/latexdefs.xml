      <tags>
        <tag id="defaultparagraph" nm="bodyText">
          <description>Default body text paragraph</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <bodyText>
              <cursor/>
              <br/>
            </bodyText>
          </initialcontents>
        </tag>
        <tag nm="rtlBodyText">
          <description>Right to left body text paragraph</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <rtlBodyText>
              <cursor/>
              <br/>
            </rtlBodyText>
          </initialcontents>
        </tag>
        <tag nm="p">
          <description>HTML paragraph</description>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <p>
              <cursor/>
              <br/>
            </p>
          </initialcontents>
        </tag>
        <tag nm="bodyMath">
          <description>Body math paragraph</description>
          <nexttag>bodyMath</nexttag>
          <discardemptyblock>true</discardemptyblock>
          <initialcontents>
            <bodyMath>
              <mml:math>
                <mml:mi tempinput="true">
                  <cursor/>
                  â€Š
                
                </mml:mi>
              </mml:math>
              <br/>
