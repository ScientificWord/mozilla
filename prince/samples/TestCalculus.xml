         <mml:mrow>
           <mml:mi mathcolor="gray">sin</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
         <mml:mspace width="thinmathspace" />
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix">&dd;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
   enter
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>u</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:msup>
         <mml:mi>x</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msup>
     </mml:mrow>
   </mml:math>
</p>
