       </mml:mrow>
       <mml:mo form="infix">+</mml:mo>
       <mml:mrow>
         <mml:mn>88</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:mn>8</mml:mn>
