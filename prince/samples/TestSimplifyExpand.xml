       </mml:mrow>
       <mml:mo form="infix">+</mml:mo>
       <mml:mn>21&ThinSpace;296</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:mn>2</mml:mn>
         </mml:msup>
         <mml:mo form="infix">+</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix" lspace="0.5em" rspace="thickmathspace" mathcolor="gray">mod</mml:mo>
       <mml:mn>2</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:mn>3</mml:mn>
         </mml:msup>
         <mml:mo form="infix">+</mml:mo>
         <mml:msup>
           <mml:mi>y</mml:mi>
           <mml:mn>6</mml:mn>
         </mml:msup>
       </mml:mrow>
       <mml:mo form="infix" lspace="0.5em" rspace="thickmathspace" mathcolor="gray">mod</mml:mo>
       <mml:mn>3</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:msup>
             <mml:mn>2</mml:mn>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:msup>
         <mml:mo form="infix">+</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
       <mml:mo form="infix" lspace="0.5em" rspace="thickmathspace" mathcolor="gray">mod</mml:mo>
       <mml:mn>2</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:msup>
             <mml:mn>2</mml:mn>
             <mml:mn>4</mml:mn>
           </mml:msup>
         </mml:msup>
         <mml:mo form="infix">+</mml:mo>
         <mml:mn>1</mml:mn>
