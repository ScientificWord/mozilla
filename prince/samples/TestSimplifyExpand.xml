       <mml:mo form="infix" lspace="0.5em" rspace="thickmathspace" mathcolor="gray">mod</mml:mo>
       <mml:mn>2</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to check equality</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mi>i</mml:mi>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>&pi;</mml:mi>
         </mml:mrow>
       </mml:msup>
       <mml:mo form="infix">=</mml:mo>
       <mml:mspace width="thinmathspace" />
       <mml:mrow>
         <mml:mo form="prefix">&minus;</mml:mo>
         <mml:mn>1</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>&pi;</mml:mi>
       <mml:mo form="infix">=</mml:mo>
       <mml:mspace width="thinmathspace" />
       <mml:mn>3.14</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi mathcolor="gray">arcsin</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi mathcolor="gray">sin</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>t</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mi>t</mml:mi>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>33</mml:mn>
         <mml:mo form="infix" lspace="0.5em" rspace="thickmathspace" mathcolor="gray">mod</mml:mo>
         <mml:mn>9</mml:mn>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mn>15</mml:mn>
         <mml:mo form="infix" lspace="0.5em" rspace="thickmathspace" mathcolor="gray">mod</mml:mo>
         <mml:mn>9</mml:mn>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:msup>
           <mml:mi>&pi;</mml:mi>
           <mml:mi>e</mml:mi>
         </mml:msup>
         <mml:mo form="infix">&minus;</mml:mo>
         <mml:msup>
           <mml:mi>e</mml:mi>
           <mml:mi>&pi;</mml:mi>
         </mml:msup>
       </mml:mrow>
       <mml:mo form="infix">=</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>&pi;</mml:mi>
             <mml:mi>e</mml:mi>
           </mml:msup>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:msup>
             <mml:mi>e</mml:mi>
             <mml:mi>&pi;</mml:mi>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">|</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
