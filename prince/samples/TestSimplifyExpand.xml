         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:mn>6</mml:mn>
         </mml:msup>
       </mml:mrow>
       <mml:mo form="infix">+</mml:mo>
       <mml:mrow>
         <mml:mn>286</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:msup>
           <mml:mi>x</mml:mi>
           <mml:mn>7</mml:mn>
