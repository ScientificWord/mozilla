<title>Test Plots</title>
</head>
<body >
<p>
   Plot 2D functions:
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mn>5</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">+</mml:mo>
       <mml:mn>3</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><br/><br/>
</p>
<p>
   <br/><br/><br/><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mo form="prefix">&minus;</mml:mo>
           <mml:mn>4</mml:mn>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">+</mml:mo>
       <mml:mn>7</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><br/><br/><mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><br/><br/><mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">cos</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><br/><br/><mml:math display="inline">
     <mml:mrow>
       <mml:mfrac>
         <mml:msup>
           <mml:mrow>
             <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
             <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
           </mml:mrow>
           <mml:mn>2</mml:mn>
         </mml:msup>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mn>2</mml:mn>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
         </mml:mrow>
       </mml:mfrac>
       <mml:mo form="infix">+</mml:mo>
       <mml:mn>7</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><br/><br/>
</p>
<p>
   Plot 3D functions
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">+</mml:mo>
       <mml:mi>y</mml:mi>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><br/><br/>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>x</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msup>
       <mml:mo form="infix">+</mml:mo>
       <mml:msup>
         <mml:mi>y</mml:mi>
         <mml:mn>2</mml:mn>
       </mml:msup>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><br/><br/>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
         <mml:mrow>
           <mml:msup>
             <mml:mi>x</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
           <mml:mo form="infix">+</mml:mo>
           <mml:msup>
             <mml:mi>y</mml:mi>
             <mml:mn>2</mml:mn>
           </mml:msup>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><br/><br/>
</p>
<p>
   <br/><br/><br/><mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi mathcolor="gray">sin</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mrow>
           <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
           <mml:mrow>
             <mml:mi>x</mml:mi>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
           <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">/</mml:mo>
       <mml:mrow>
         <mml:mo form="prefix" fence="true" stretchy="false">(</mml:mo>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mi>y</mml:mi>
         </mml:mrow>
         <mml:mo form="postfix" fence="true" stretchy="false">)</mml:mo>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <br/><br/><br/>
</p>
</body>

