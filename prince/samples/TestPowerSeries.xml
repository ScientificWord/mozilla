<body >
<h1 class="section">Some expressions for Power Series</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">sin</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>
   use
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>
   at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>5</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>e</mml:mi>
         <mml:mrow>
           <mml:mn>2</mml:mn>
           <mml:mo>&InvisibleTimes;</mml:mo>
           <mml:mi>n</mml:mi>
         </mml:mrow>
       </mml:msup>
     </mml:mrow>
   </mml:math>
   use
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>n</mml:mi>
     </mml:mrow>
   </mml:math>
   at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>0</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>10</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi mathcolor="gray">ln</mml:mi>
       <mml:mo>&ApplyFunction;</mml:mo>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>
   use
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
     </mml:mrow>
   </mml:math>
   at
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>1</mml:mn>
     </mml:mrow>
   </mml:math>
   and
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>5</mml:mn>
     </mml:mrow>
   </mml:math>
</p>
</body>

