           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">&minus;</mml:mo>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">sec</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">csc</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to rewrite tan</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mrow>
           <mml:mi mathcolor="gray">cos</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">sin</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="infix">&minus;</mml:mo>
       <mml:mrow>
         <mml:mn>2</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">sec</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mrow>
           <mml:mi mathcolor="gray">csc</mml:mi>
           <mml:mo>&ApplyFunction;</mml:mo>
           <mml:mi>x</mml:mi>
         </mml:mrow>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to rewrite arcsin</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi mathcolor="gray">arccos</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&minus;</mml:mo>
       <mml:mrow>
         <mml:mi mathcolor="gray">arctan</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to rewrite arccos</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi mathcolor="gray">arctan</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&minus;</mml:mo>
       <mml:mrow>
         <mml:mi mathcolor="gray">arccot</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to rewrite arctan</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi mathcolor="gray">arccot</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&minus;</mml:mo>
       <mml:mrow>
         <mml:mi mathcolor="gray">arcsin</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to rewrite arccot</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mi mathcolor="gray">arcsin</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
       <mml:mo form="infix">&minus;</mml:mo>
       <mml:mrow>
         <mml:mi mathcolor="gray">arccos</mml:mi>
         <mml:mo>&ApplyFunction;</mml:mo>
         <mml:mi>x</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to rewrite polar</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mn>3</mml:mn>
       <mml:mo form="infix">+</mml:mo>
       <mml:mrow>
         <mml:mn>5</mml:mn>
         <mml:mo>&InvisibleTimes;</mml:mo>
         <mml:mi>i</mml:mi>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to rewrite rectangular</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mroot>
         <mml:mi>i</mml:mi>
         <mml:mn>3</mml:mn>
       </mml:mroot>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:msup>
         <mml:mi>i</mml:mi>
         <mml:mfrac>
           <mml:mn>1</mml:mn>
           <mml:mn>3</mml:mn>
         </mml:mfrac>
       </mml:msup>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to rewrite normal form</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>3</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>x</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mrow>
                 <mml:mn>3</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>x</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>8</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>x</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mn>7</mml:mn>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
       <mml:mo form="infix">+</mml:mo>
       <mml:mrow>
         <mml:mstyle displaystyle="true" scriptlevel="0">
           <mml:mfrac>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>5</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>x</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mn>3</mml:mn>
             </mml:mrow>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:msup>
                   <mml:mi>x</mml:mi>
                   <mml:mn>2</mml:mn>
                 </mml:msup>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mn>7</mml:mn>
             </mml:mrow>
           </mml:mfrac>
         </mml:mstyle>
       </mml:mrow>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to rewrite equations as matrix</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">{</mml:mo>
       <mml:mrow>
         <mml:mrow>
           <mml:mi>x</mml:mi>
           <mml:mo form="infix">+</mml:mo>
           <mml:mrow>
             <mml:mn>2</mml:mn>
             <mml:mo>&InvisibleTimes;</mml:mo>
             <mml:mi>y</mml:mi>
           </mml:mrow>
           <mml:mo form="infix">&minus;</mml:mo>
           <mml:mn>3</mml:mn>
         </mml:mrow>
         <mml:mo form="infix">,</mml:mo>
         <mml:mrow>
           <mml:mrow>
             <mml:mrow>
               <mml:mn>3</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>x</mml:mi>
             </mml:mrow>
             <mml:mo form="infix">&minus;</mml:mo>
             <mml:mrow>
               <mml:mn>5</mml:mn>
               <mml:mo>&InvisibleTimes;</mml:mo>
               <mml:mi>y</mml:mi>
             </mml:mrow>
           </mml:mrow>
           <mml:mo form="infix">=</mml:mo>
           <mml:mn>0</mml:mn>
         </mml:mrow>
       </mml:mrow>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">}</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
       <mml:mtable align="axis" columnalign="left">
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mi>x</mml:mi>
               <mml:mo form="infix">+</mml:mo>
               <mml:mi>y</mml:mi>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>z</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>x</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>4</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>y</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">+</mml:mo>
               <mml:mi>z</mml:mi>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mrow>
               <mml:mrow>
                 <mml:mn>2</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>y</mml:mi>
               </mml:mrow>
               <mml:mo form="infix">&minus;</mml:mo>
               <mml:mrow>
                 <mml:mn>3</mml:mn>
                 <mml:mo>&InvisibleTimes;</mml:mo>
                 <mml:mi>z</mml:mi>
               </mml:mrow>
             </mml:mrow>
             <mml:mo form="infix">=</mml:mo>
             <mml:mrow>
               <mml:mo form="prefix">&minus;</mml:mo>
               <mml:mn>1</mml:mn>
             </mml:mrow>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
     </mml:mrow>
   </mml:math>
</p>
<h1 class="section">Some expressions to rewrite matrix as equations</h1>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">(</mml:mo>
       <mml:mtable align="axis">
         <mml:mtr>
           <mml:mtd>
             <mml:mn>1</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>1</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mn>2</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>3</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>1</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">)</mml:mo>
     </mml:mrow>
   </mml:math>
   enter
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">,</mml:mo>
       <mml:mi>y</mml:mi>
     </mml:mrow>
   </mml:math>
</p>
<p>
   <mml:math display="inline">
     <mml:mrow>
       <mml:mo form="prefix" fence="true" stretchy="true" symmetric="true">[</mml:mo>
       <mml:mtable align="axis">
         <mml:mtr>
           <mml:mtd>
             <mml:mn>1</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>1</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>2</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>1</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mn>2</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>4</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>1</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>0</mml:mn>
           </mml:mtd>
         </mml:mtr>
         <mml:mtr>
           <mml:mtd>
             <mml:mn>0</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mn>2</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>3</mml:mn>
           </mml:mtd>
           <mml:mtd>
             <mml:mo form="prefix">&minus;</mml:mo>
             <mml:mn>1</mml:mn>
           </mml:mtd>
         </mml:mtr>
       </mml:mtable>
       <mml:mo form="postfix" fence="true" stretchy="true" symmetric="true">]</mml:mo>
     </mml:mrow>
   </mml:math>
   enter
   <mml:math display="inline">
     <mml:mrow>
       <mml:mi>x</mml:mi>
       <mml:mo form="infix">,</mml:mo>
       <mml:mi>y</mml:mi>
       <mml:mo form="infix">,</mml:mo>
       <mml:mi>z</mml:mi>
     </mml:mrow>
   </mml:math>
</p>
