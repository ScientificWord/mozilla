                      <menuitem label="&default.label;" def="1"/>
                      <menuitem value="letter" label="&LetterSize.label;"/>
                      <menuitem value="a4paper" label="&A4Size.label;"/>
                      <menuitem value="a5paper" label="&A5Size.label;"/>
                      <menuitem value="b5paper" label="&B5Size.label;"/>
                      <menuitem value="legalpaper" label="&LegalSize.label;"/>
