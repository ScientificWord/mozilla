
DEPTH		= ../../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@


include $(DEPTH)/config/autoconf.mk
include $(topsrcdir)/config/rules.mk

SHELL_RESOURCES := $(wildcard	$(srcdir)/*.sci  $(srcdir)/*.xhtml) \
	$(NULL)


libs::	$(SHELL_RESOURCES)
