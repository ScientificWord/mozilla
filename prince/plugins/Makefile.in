
DEPTH		= ../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@


include $(DEPTH)/config/autoconf.mk
include $(topsrcdir)/config/rules.mk

PLUGIN_LIBS := $(wildcard	$(srcdir)/*.dll)
PLUGIN_LIBS += $(wildcard	$(srcdir)/*.xpt)
PLUGIN_LIBS += $(wildcard	$(srcdir)/*.class)
PLUGIN_LIBS += $(wildcard	$(srcdir)/*.so)

libs::	$(PLUGIN_LIBS) 
	$(INSTALL)	$(PLUGIN_LIBS)	$(FINAL_TARGET)/plugins





	
