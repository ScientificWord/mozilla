\c@secnumdepth=4

\ps@footings\let\chaptermark\@gobble\let\sectionmark\@gobble
\def\ps@chapter{}%
\let\rulefooting\norulefooting
\let\headcase\@empty
\def\rectomark{\rightmark}%
\def\versomark{\leftmark}%

\def\@makechapterhead#1{%
 {\raggedright\advance\leftskip40pt
  \huge\em
  \ifnum \c@secnumdepth >\m@ne \@chapapp{} \thechapter \quad \fi
  #1\par 
  \vskip4pt\nointerlineskip
  }%
 \nobreak \vskip 10pt
 }
\def\@makeschapterhead#1{%
 {\raggedright\advance\leftskip40pt
  \huge\em
  %\ifnum \c@secnumdepth >\m@ne \@chapapp{} \thechapter \quad \fi
  #1\par 
  \vskip4pt\nointerlineskip
  }%
 \nobreak \vskip 10pt
 }
\let\introtext\@empty

\def\section{%
 \@startsection {section}{1}{\z@}{-10pt plus -1.2ex minus -.2ex}%
  {.5pt}{\large\bf}%
 }%
\def\subsection{%
 \@startsection{subsection}{2}{\z@}{-10pt plus -1ex minus -.2ex}%
  {.5pt}{\bf}%
 }%

\def\thefigure{\@arabic\c@figure}%

%Turn off the footers in the bibliography
%Copy from report.cls, except for the %'ed line below
\renewenvironment{thebibliography}[1]
     {\chapter*{\bibname}%
      %\@mkboth{\MakeUppercase\bibname}{\MakeUppercase\bibname}%
      \list{\@biblabel{\@arabic\c@enumiv}}%
           {\settowidth\labelwidth{\@biblabel{#1}}%
            \leftmargin\labelwidth
            \advance\leftmargin\labelsep
            \@openbib@code
            \usecounter{enumiv}%
            \let\p@enumiv\@empty
            \renewcommand\theenumiv{\@arabic\c@enumiv}}%
      \sloppy
      \clubpenalty4000
      \@clubpenalty \clubpenalty
      \widowpenalty4000%
      \sfcode`\.\@m}
     {\def\@noitemerr
       {\@latex@warning{Empty `thebibliography' environment}}%
      \endlist}
