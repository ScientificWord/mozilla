%***************************************************************************%
%                                                                           %
%                      The Footnotes Marks                                  %
%                                                                           %
%***************************************************************************%
%
\long\def\@makefntext#1{\def\baselinestretch{1}\vskip -.18in
\vskip 0pt\parindent .25in\noindent 
 \hbox to .8em{ }#1}
%
%***************************************************************************%
%                                                                           %
%                               Quotation                                   %
%                                                                           %
%***************************************************************************%
%
\def\quotation{\list{}{\listparindent 0.0em %1.5em
    \itemindent\listparindent
    \rightmargin\leftmargin \parsep \z@ plus\p@}\item[]}
\let\endquotation=\endlist
%
%***************************************************************************%
%                                                                           %
%                               Footnotes                                   %
%                                                                           %
%***************************************************************************%
%
\long\def\@footnotetext#1{\def\baselinestretch{1}
   \insert\footins{\footnotesize
    \interlinepenalty\interfootnotelinepenalty 
    \splittopskip\footnotesep
    \splitmaxdepth \dp\strutbox \floatingpenalty \@MM
    \hsize\columnwidth \@parboxrestore
   \edef\@currentlabel{\csname p@footnote\endcsname}\@makefnmark \@makefntext
    {\rule{\z@}{\footnotesep}\ignorespaces
      #1\@finalstrut\strutbox}}}
%
%***************************************************************************%
%                                                                           %
%                             References                                    %
%                               without                                     %
%                               brackets                                    %
%                                                                           %
%***************************************************************************%
%
\def\@biblabel#1{#1\hfil}%
\def\thebibliography#1{\newpage\section*{\large \bf REFERENCES\@mkboth
 {\large \bf REFERENCES}{\large \bf REFERENCES}}\list
 {}{\settowidth\labelwidth{#1}\labelwidth\leftmargin
 \usecounter{enumi}
 }
 \def\newblock{\hskip .11em plus .33em minus .07em}
 \sloppy\clubpenalty4000\widowpenalty4000
 \sfcode`\.=1000\relax\def\baselinestretch{1}\large \normalsize}
\let\endthebibliography=\endlist
