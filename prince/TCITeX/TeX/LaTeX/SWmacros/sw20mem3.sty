\ps@memo
\def\@memohead{%
 \vbox{%
  \vskip\z@%Extra space between top of page and memodept
  \@tempswafalse
  \ifx\@empty\the@memocompany\else\@tempswatrue\fi
  \ifx\@empty\the@memoname\else\@tempswatrue\fi
  \ifx\@empty\the@memodept\else\@tempswatrue\fi
  \if@tempswa
   \begin{center}\@tempswafalse
    \ifx\@empty\the@memocompany
    \else\if@tempswa\\\fi
    \@tempswatrue{\bf\letparBL\the@memocompany\vspace{.1in}}\fi
    \ifx\@empty\the@memoname
    \else\if@tempswa\\\fi\@tempswatrue{\letparBL\the@memoname\vspace{.1in}}\fi
    \ifx\@empty\the@memodept
    \else\if@tempswa\\\fi\@tempswatrue{\letparBL\the@memodept\vspace{.1in}}\fi
   \end{center}%
  \fi
  \vskip\z@%Extra space between memodept and 'Memorandum' box
  \hrule height4pt{\LARGE\bf Memorandum\par}%
  \vskip24pt\hrule height1pt\vskip15pt
  \jot\z@
  \@memoalign
 }\nobreak\vskip24pt\raggedright
}%

