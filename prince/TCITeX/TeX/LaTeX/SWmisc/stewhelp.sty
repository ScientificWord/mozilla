\def\eatdollar#1{}
     \let\BLACKSQUARE\blacksquare
     \def\blacksquare{%
            \ifinner
               \hfill\BLACKSQUARE$%
            \else 
               \]%
               \vskip -\baselineskip
               \vskip -\belowdisplayshortskip
               \hfill$\BLACKSQUARE$
            \fi
            \eatdollar
     }

