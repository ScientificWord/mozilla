}
\def\@appendix#1#2{
\def\a@title{#2}
\ifx\@empty\a@title % no title
%\@startsection {section}{1}{1em}{\@beforeskip}%
%{\@afterskip}{\centering\normalsize}*{\bf Appendix#1}
%\@startsection {section}{1}{\z@}{2.5ex plus 1ex minus 
% .2ex}{2.5ex plus .2ex}{\normalsize\bf}*{Appendix#1}
\@startsection {section}{1}{\z@}{2.5ex plus 1ex minus 
.2ex}{.001pt}{\normalsize\bf}*{Appendix#1}


%{\@afterskip}{\centering\small}*{APPENDIX#1}
\else % with title
  \if@noskipsec \leavevmode \fi
  \par \@afterindenttrue
  \if@nobreak \everypar{}\else
    \addpenalty{\@secpenalty}\addvspace{\@beforeskip}\fi
  \@hangfrom{\hspace*{1em}{\sf Appendix}#1:~}{\interlinepenalty \@M #2\par}
  \@xsect{\@afterskip}
\fi
  \setcounter{section}{0}
  \def\thesection{}% section should not be used in appendix
  \setcounter{subsection}{0}
  \def\thesubsection{\theappendix.\arabic{subsection}}
  \setcounter{subsubsection}{0}
  \def\thesubsubsection{\thesubsection.\arabic{subsubsection}}
  \setcounter{equation}{0}
  \def\theequation{{\rm \theappendix.\arabic{equation}}}
}

