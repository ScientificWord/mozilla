\def\@startsubsection#1#2#3#4#5#6#7{\if@noskipsec \leavevmode \fi
   \par \@tempskipa #4\relax
   \@afterindenttrue
   \ifdim \@tempskipa <\z@ \@tempskipa -\@tempskipa \@afterindentfalse\fi
   \if@nobreak \everypar{}\else
     \addpenalty{\@secpenalty}\addvspace{\@tempskipa}\fi \@ifstar
     {\@subssect{#3}{#4}{#5}{#6}{#7}}{\@dblarg{\@subsect{#1}{#2}{#3}{#4}{#5}{#6}{#7}}}}
     
\def\@subsect#1#2#3#4#5#6#7[#8]#9{\ifnum #2>\c@secnumdepth
     \def\@svsec{}\else
%     \refstepcounter{#1}\edef\@svsec{\csname the#1\endcsname\hskip 1em}\fi
     \refstepcounter{#1}\edef\@svsec{\csname the#1\endcsname\n}\fi
     \@tempskipa #5\relax
      \ifdim \@tempskipa>\z@
        \begingroup #6\relax
          \@hangfrom{\hskip #3\relax\@svsec}
          {\interlinepenalty \@M #7#9\par}%
%          {\interlinepenalty \@M
%             \setbox\@tempboxa=\hbox{#9}
%             \ifdim \wd\@tempboxa>\hsize
%                \leftskip=\wd\@svsec \unhbox\@tempboxa
%                 \lwftskip=16pt  \unhbox\@tempboxa
%                \leftskip=0pt
%             \else #9
%             \fi
%           \par}%
         \endgroup
        \csname #1mark\endcsname{#8}\addcontentsline
          {toc}{#1}{\ifnum #2>\c@secnumdepth 
                    \else \protect\numberline{\csname the#1\endcsname}\fi
                     #8}
                    \else
                      \def\@svsechd{#6\hskip #3\@svsec #7#9\csname #1mark\endcsname
                        {#8}\addcontentsline
                             {toc}{#1}{\ifnum #2>\c@secnumdepth \else
                               \protect\numberline{\csname the#1\endcsname}\fi
                         #8}}\fi
        \@xsect{#5}}
        
\def\@subssect#1#2#3#4#5#6{\@tempskipa #3\relax
   \ifdim \@tempskipa>\z@
     \begingroup #4\@hangfrom{\hskip #1}{\interlinepenalty \@M #5#6\par}\endgroup
   \else \def\@svsechd{#5\hskip #1\relax #6}\fi
     \@xsect{#3}}

\def\@hangfrom#1{\setbox\@tempboxa=\hbox{#1}%
      \hangindent 1.3\wd\@tempboxa\noindent\box\@tempboxa}


