\def\abstractpage
   {\newpage
    \thispagestyle{plain}
    \vspace*{0.5in}
    \begin{center}
       {\def\baselinestretch{1.2}
                         \Large\bf ABSTRACT \par\par}
                            {\large{\bf \@title} \\
                                     by \\
                                  \@author \\
                            }
                      \def\baselinestretch{1}\@normalsize
                      \Unh, \@degreemonth, \@degreeyear
    \end{center}
    \par
    \begin{abstract}
   }

\def\endabstractpage
   {\end{abstract}
    \newpage
   }



