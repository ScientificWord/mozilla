DEPTH		= ../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@




include $(DEPTH)/config/autoconf.mk
include $(topsrcdir)/config/rules.mk

MACROS	 := $(wildcard	$(srcdir)/TeX/LaTeX/*)

libs::	$(MACROS)
	if test ! -d $(FINAL_TARGET)/TCItex; then mkdir $(FINAL_TARGET)/TCItex; fi
	-cp -R	${srcdir}/TeX/LaTeX/*	$(FINAL_TARGET)/TCItex